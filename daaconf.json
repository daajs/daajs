{
   compiler: {
         sourcepath: "pages",
         releasepath: "pages",
         include: "\\.daa$",      
         exclude: "^_|debug|version"
   },
   builder: {
      pages: {
         sourcepath: "pages",
         releasepath: "",
         include: "\\.json$",      
         exclude: "^_|debug|version"
      },
      templates: {
         sourcepath: "templates",
         releasepath: "",
         include: "\\.(html|php)$",      
         exclude: "^_|debug|version" 
      },
      css: {
         sourcepath: "css",
         releasepath: "",
         releasename: "style.css",
         include: "\\.css$",
         exclude: "^_|debug|version|reset",
         isjoined: 0,
         isminimized: 0
      },
      modules: {
         sourcepath: "js",
         include: "\\.js$",      
         exclude: "^_|debug|version|^min$",
         requires: "daa",
         releasepath: "",
         releasename: "daalib.js",
         releasesuffix: "min",
         releasetext: "/*\n DAAJS v0.9.2\n (c) 2012-2016 Mikaella Klyueva, DAA Systems. http://daajs.org\n License: MIT\n*/\n",
         isjoined: 0,
         isminimized: 0,
         islines: 0
      }
   }
}